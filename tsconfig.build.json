{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "declaration": false,
    "declarationMap": false,
    "sourceMap": false,
    "incremental": false,
    "composite": false,
    "skipLibCheck": true,
    "noEmit": false
  },
  "exclude": [
    "node_modules",
    "dist",
    "test",
    "**/*.test.ts",
    "**/*.spec.ts",
    "*.js",
    "simple-agent.js",
    "test-*.js"
  ]
}
